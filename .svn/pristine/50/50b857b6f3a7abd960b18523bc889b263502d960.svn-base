@import './variables.scss';
// 登录背景色
html,body{
    font-weight: $font-weight;
}
.login-container {
    background-color: $--color-white;
}
.body,.main,.sider{
    background-color: $--color-white;
}
// logo图片
.logo-box{
    background:  $logo-background;
    transition: all 0.3s cubic-bezier(0.645, 0.045, 0.355, 1);
}